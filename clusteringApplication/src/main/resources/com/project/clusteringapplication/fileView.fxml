<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="265.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.clusteringapplication.FileViewController">
   <children>
      <Label layoutX="48.0" layoutY="87.0" text="Current Dataset:" textFill="#0b709b">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Text fx:id="fileNameText" layoutX="180.0" layoutY="102.0" strokeType="OUTSIDE" strokeWidth="0.0" text="filename" wrappingWidth="136.3515625" />
      <Label layoutX="48.0" layoutY="126.0" text="Number of features:" />
      <Text fx:id="featureText" layoutX="180.0" layoutY="139.0" strokeType="OUTSIDE" strokeWidth="0.0" text="featuers" wrappingWidth="88.3515625" />
      <Button fx:id="clusterSettingsButton" layoutX="36.0" layoutY="212.0" mnemonicParsing="false" onAction="#openSettings" style="-fx-background-color: #0b709b;" text="Cluster settings" textFill="WHITE" />
      <Button fx:id="newFileButton" layoutX="268.0" layoutY="212.0" mnemonicParsing="false" onAction="#chooseNewFile" text="Select new file" />
      <Label alignment="CENTER" layoutX="48.0" layoutY="161.0" text="Number of samples:" />
      <Text fx:id="sampleText" layoutX="180.0" layoutY="174.0" strokeType="OUTSIDE" strokeWidth="0.0" text="samples" wrappingWidth="136.3515625" />
      <Label layoutX="103.0" layoutY="28.0" text="Data set infromation" textFill="#0b709b">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="returnButton" layoutX="146.0" layoutY="212.0" mnemonicParsing="false" onAction="#returnToStart" prefHeight="25.0" prefWidth="108.0" style="-fx-background-color: e40a0a;" text="Return to start" textFill="WHITE" />
   </children>
</AnchorPane>
