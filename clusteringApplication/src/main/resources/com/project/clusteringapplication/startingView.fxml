<AnchorPane prefHeight="184.0" prefWidth="477.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.clusteringapplication.StartViewController">
   <children>
   
          <Label fx:id="welcomeText" layoutX="62.0" layoutY="42.0" text="ZHAC081 Data Clustering Application" textFill="#0b709b">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
       <Button fx:id="startButton" layoutX="33.0" layoutY="106.0" onAction="#chooseFile" prefHeight="47.0" prefWidth="117.0" style="-fx-background-color: #0b709b;" text="Begin clustering" textFill="WHITE" />
      <Button fx:id="pluginButton" layoutX="166.0" layoutY="106.0" mnemonicParsing="false" onAction="#openPluginManager" prefHeight="47.0" prefWidth="117.0" text="Select plugin folder" textAlignment="CENTER" wrapText="true" />
      <Label layoutX="299.0" layoutY="106.0" text="Selected parser" />
      <ComboBox fx:id="parserSelect" layoutX="299.0" layoutY="128.0" prefWidth="150.0" />
   </children>
</AnchorPane>
